{
    "schedules": [

    {
        "type": "program",
        "name": "nas-monitor",
        "program_class": "processmanager.programs.nas_program.NASProgram",
        "keep_alive": true,
        "check_alive_freq": "10 m",
        "max_retries": 10
    },
    {
        "type": "program",
        "name": "tws",
        "program_class": "processmanager.programs.tws_program.TWS_Program",
        "keep_alive": true,
        "check_alive_freq": "5 m",
        "max_retries": 9999999
    },
    {
        "type": "program",
        "name": "data-server",
        "program_class": "processmanager.programs.data_server_program.DataServerProgram",
        "keep_alive": true,
        "check_alive_freq": "1 m",
        "max_retries": 10
    }, 
    {
        "type": "program",
        "name": "mongo-daemon",
        "program_class": "processmanager.programs.mongo_program.MongoProgram",
        "keep_alive": true,
        "check_alive_freq": "1 m",
        "max_retries": 10
    },

    {
        "type": "task",
        "name": "update-strategy-allocation",
        "func": "sysproduction.update_strategy_capital.update_strategy_capital",
        "start": "00:45 am pst"
    },

    {
        "type": "task",
        "name": "update total capital",
        "func": "processmanager.tasks.update_total_capital_wrapper.update_total_capital_wrapper",
        "start": "00:45 am pst",
        "freq": "1 h"
    },

    {
        "type": "task",
        "name": "do backups",
        "func": "processmanager.tasks.backup_and_reboot.backup",
        "start": "11:59 pm pst"
    }
    ]
  }